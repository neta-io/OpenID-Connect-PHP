{
  "name": "neta-io/openid-connect-php",
  "description": "Bare-bones OpenID Connect client",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.4",
    "phpseclib/phpseclib": "~2.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "roave/security-advisories": "dev-master"
  },
  "archive": {
    "exclude": [
      ".*"
    ]
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "repositories": {
    "packagist": false,
    "artifactoryNeta": {
      "type": "composer",
      "url": "https://artifactory.ops.neta.sh/api/composer/php-local"
    },
    "artifactoryCache": {
      "type": "composer",
      "url": "https://artifactory.ops.neta.sh/api/composer/php-remote"
    }
  }
}
